{
  "name": "ttp-lab-1",
  "version": "1.0.0",
  "main": "src/server/index.ts",
  "repository": "git@github.com:junkkerrigan/ttp-lab-1.git",
  "author": "Andrew Pavlenko <paavlenko.andrew@gmail.com>",
  "license": "MIT",
  "scripts": {
    "open": "open 'http://localhost:8080'",
    "clean": "rm -rf ./dist",
    "watch:server": "webpack --watch --config webpack.config.server.js",
    "watch:client": "webpack --watch --config webpack.config.client.js",
    "tab:watch:server": "ttab -t 'Watch server' npm run watch:server",
    "tab:watch:client": "ttab -t 'Watch client' npm run watch:client",
    "prewatch": "npm run clean",
    "watch": "run-s tab:watch:client tab:watch:server open",
    "prebuild": "npm run clean",
    "build:server": "webpack --config webpack.config.server.js",
    "build:client": "webpack --config webpack.config.client.js",
    "build": "npm run build:server && npm run build:client",
    "serve": "node ./dist/server.js",
    "start": "npm run build && npm run serve && npm run open",
    "lint": "eslint ./src/* --no-error-on-unmatched-pattern",
    "lint:fix": "eslint ./src/* --fix --no-error-on-unmatched-pattern"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint:fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "antd": "^4.2.4",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.6.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.21.6"
  },
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.2.0",
    "@types/classnames": "^2.2.10",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/node": "^13.7.7",
    "@types/passport": "^1.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/sequelize": "^4.28.8",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-yoshi": "^4.78.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-wix-style-react": "^1.0.412",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.2",
    "nodemon-webpack-plugin": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.1",
    "ttab": "^0.6.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-livereload-plugin": "^2.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "extends": "yoshi"
  }
}
